{
  "name": "keez-api-npm-package",
  "version": "1.0.0",
  "description": "Keez API wrapper using Node",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.(js|ts)\"",
    "lint": "eslint src --ext .js,.ts",
    "lint:fix": "eslint src --fix --ext .js,.ts",
    "test": "jest --config jest.config.js",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TPN-Labs/keez-node.git"
  },
  "keywords": [
    "keez",
    "node",
    "typescript"
  ],
  "author": "Mihai-Alexandru Matraguna",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TPN-Labs/keez-node/issues"
  },
  "homepage": "https://github.com/TPN-Labs/keez-node#readme",
  "devDependencies": {
    "@types/jest": "29.4.0",
    "@typescript-eslint/eslint-plugin": "5.54.0",
    "@typescript-eslint/parser": "5.52.0",
    "eslint": "8.35.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-jest": "27.2.1",
    "jest": "29.4.3",
    "prettier": "2.8.4",
    "ts-jest": "29.0.5",
    "typescript": "4.9.5"
  },
  "files": [
    "dist/**/*"
  ]
}
